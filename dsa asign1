#include<stdio.h>
#include<string.h>
int main(){
	 char s[30];
	 int i,j,k,l,m,n,o,p;
 	scanf("%s",s);
 	l=strlen(s);
if(!strcmp(s,"goodbye")){
 		printf("yes");
 		
 	}
 	else if(l<8)
 	printf("NO");
 	for(i=0;i<l;i++){
 		if(s[i]=='g'&&(l-i)>5){
 			for(j=i+1;j<l;j++){
 				if(s[j]=='o'&&(l-j)>4){
 				for(k=j+1;k<l;k++){
 					if(s[k]=='o'&&(l-k)>3){
 					for(m=k+1;m<l;m++){
 						if(s[m]=='d'&&(l-m)>2){
 						for(n=m+1;n<l;n++){
 							if(s[n]=='b'&&(l-n)>1){
 							 for(o=n+1;o<l;o++){
 							 	if(s[o]=='y'&&(l-o)>0){
 							 		for(p=o+1;p<l;p++){
 							 			if(s[p]=='e'){
 							 				printf("yes\n");
 							 				return 0;
 							 			}
 							 	     }
 							 	}
 							 }
 						}
 				     }
 	               }
 			}
 			}
 		}}}}}
 		printf("no");
 		return 0;
 	}
 							
 					
 			
